<%= link_to('Login with Google!', '/auth/google_oauth2') %>
<h2>Login</h2>
<%= form_tag '/login' do %>
    <%= text_field_tag :username, nil, :placeholder => "Enter username"  %> <br><br>
    <%= password_field_tag :password, nil, :placeholder => "Enter password" %><br><br>
    <%= submit_tag "Login" %>
<% end %>